{
  "development": {
    "scripts": [
      "/public/src/components/requirejs/require.js"
    ],
    "css": [
      "/public/src/components/bootstrap/docs/assets/css/bootstrap.css",
      "/public/src/css/styles.css"
    ],
    "requirejs": {
      "baseUrl": "/src/js",
      "paths": {
        "jquery": "../components/jquery/jquery",
        "backbone": "../components/backbone/backbone",
        "globalize": "../components/globalize/lib/globalize",
        "lodash": "../components/lodash/lodash"
      },
      "shim": {
        "globalize": {
          "deps": ["jquery"],
          "exports": "Globalize"
        },
        "director": {
          "deps": ["jquery"],
          "exports": "Router"
        }
      }
    }
  },
  "production": {
    "scripts": [
      "/public/build/js/scripts.min.js"
    ],
    "css": [
      "/public/build/css/styles.min.css"
    ]
  }
}
